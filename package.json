{
  "name": "zeta-web",
  "files": [
    "dist"
  ],
  "main": "./index.js",
  "module": "./index.js",
  "exports": {
    ".": {
      "import": "./dist/zeta-web.js",
      "require": "./dist/zeta-web.umd.cjs"
    },
    "./tokens.css": {
      "import": "./tokens.css",
      "require": "./tokens.css"
    },
    "./index.css": {
      "import": "./index.css",
      "require": "./index.css"
    }
  },
  "version": "0.1.0",
  "description": "Zebra Zeta Design System - Web Components",
  "repository": "https://github.com/zebratechnologies/zeta-web",
  "author": "Ben Kennedy",
  "license": "MIT",
  "type": "module",
  "declaration": "true",
  "declarationMap": "true",
  "customElements": "custom-elements.json",
  "scripts": {
    "analyze": "cem analyze",
    "analyze:watch": "cem analyze --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "docs": "npx typedoc",
    "lint": "eslint --ignore-path .gitignore --ext .ts,.js \"**/*.{ts,js}\"",
    "lintnew": "eslint -f unix \"src/**/*.{ts,tsx}\"",
    "lint:lit-analyzer": "lit-analyzer --format markdown --outFile result.md",
    "prettier": "prettier {*,src/*,src/**/*} --write --ignore-unknown",
    "start": "web-dev-server --node-resolve --open ./demo/ --watch",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "test": "web-test-runner \"src/test/**/*.test.ts\" --node-resolve",
    "test:watch": "web-test-runner \"src/test/**/*.test.ts\" --node-resolve --watch",
    "build-storybook": "npx vite build && storybook build",
    "build-storybook:with-output-dir": " npx vite build && storybook build -o ",
    "react-gen": "npx vite build && cd scripts && node reactify.js",
    "create": "cd scripts && node makeWebComponentTemplate.js",
    "localize:extract": "lit-localize extract",
    "localize:build": "lit-localize build"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.3",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@lit/localize-tools": "^0.7.2",
    "@open-wc/lit-helpers": "^0.7.0",
    "@open-wc/testing": "^3.0.0-next.5",
    "@remcovaes/web-test-runner-vite-plugin": "^1.2.1",
    "@storybook/addon-a11y": "^8.0.0",
    "@storybook/addon-designs": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/theming": "^8.0.0",
    "@storybook/web-components": "^8.0.0",
    "@storybook/web-components-vite": "^8.0.0",
    "@types/dom-speech-recognition": "^0.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@web/dev-server": "^0.4.2",
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-playwright": "^0.11.0",
    "axe-playwright": "^1.2.3",
    "cem-plugin-better-lit-types": "^0.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-wc": "^2.0.4",
    "lit-analyzer": "^2.0.3",
    "prettier": "3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss-lit": "^2.1.0",
    "sass": "^1.70.0",
    "storybook": "^8.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.1",
    "vite-dts": "^1.0.4",
    "vite-plugin-cp": "^4.0.8",
    "vite-plugin-dts": "^3.7.2"
  },
  "dependencies": {
    "@fontsource/ibm-plex-sans": "^5.0.18",
    "@lit/localize": "^0.12.1",
    "@zebra-fed/zeta-icons": "^0.1.66",
    "firebase-tools": "^13.5.2",
    "lit": "^3.0.0"
  },
  "overrides": {
    "rollup-plugin-scss-lit": {
      "lit": "^3.0.0"
    }
  },
  "resolutions": {
    "react-router-dom": "^6.3.0",
    "react-router": "^6.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
